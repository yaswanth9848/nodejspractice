<style>
    table, th, td {
      border: 1px solid rgb(138, 12, 12);
      border-collapse: collapse;
    }
    th, td {
      background-color: #96D4D4;
    }
</style>


<table>
    <tr>
       <th>name</th>
       <th>company</th>
       <th>FuelType</th>
       <th>capacity</th>
    </tr>
 
    <% for (let i = 0; i < data.length; i++) { %>
     <tr>
       <td style="color:rgb(162, 0, 255)"><%= data[i].name %></td>
       <td><%= data[i].company %></td>
       <td><%= data[i].FuelType %></td>
       <td><%= data[i].capacity %></td>
     </tr>    
    <% } %>
 </table>
